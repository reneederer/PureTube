{
  "manifest_version": 2,
  "name": "PureTube",
  "version": "1.0.1",

  "description": "Removes searchbar, comments, recommendations, shorts from youtube",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": [ "*://*.youtube.com/*", "*://*.youtube.de/*" ],
      "js": ["PureTube.js"]
    }
  ]
}